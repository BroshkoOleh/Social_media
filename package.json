    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
